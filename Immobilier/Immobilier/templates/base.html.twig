<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Immo 2020{% endblock %}</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset("/libs/css/main.css") }}">
    {% endblock %}
</head>
<body>
{% block header %}
    <header class="w-100 border-bottom border-dark">
        <nav class="navbar navbar-expand-sm navbar-dark">
            <a class="navbar-brand" href="{{ path('home') }}">
                <img src="/images/logo/logo.jpg" id="logo"  alt="Logo_immo">
                <small id="Titre" class="text-dark">Immo 2020</small>
            </a>
            <button class="navbar-toggler bg-dark" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link text-dark menu" href="{{ path('home') }}">Accueil</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark menu" href="{{ path('logement_index') }}">Toutes les offres</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark menu" href="{{ path('contact') }}">Contact</a>
                    </li>
{#                    {% if app.user %}#}
{#                    <li class="nav-item">#}
{#                        <a class="nav-link text-dark menu" href="{{ path('app_logout') }}">Deconnexion</a>#}
{#                        {% else %}#}
{#                    </li>#}
{#                    <li class="nav-item">#}
{#                        <a class="nav-link text-dark menu" href="{{ path('app_login') }}">Connexion</a>#}
{#                    </li>#}
{#                    {% endif %}#}
                </ul>

            </div>
        </nav>
    </header>


{% endblock %}
<div class="container mt-5">
    {% block body %}{% endblock %}
</div>




{% block footer %}
    <!-- Footer -->
    <footer class="page-footer font-small stylish-color-dark pt-3 bg-dark">

        <!-- Footer Links -->
        <div class="container text-center text-md-left">

            <!-- Grid row -->
            <div class="row">

                <!-- Grid column -->
                <div class="col-md-6 mx-auto">

                    <!-- Content -->
                    <h5 class="font-weight-bold text-uppercase mt-3 mb-2 text-light">Immo 2020</h5>
                    <small class="text-light">Ouvert du lundi au vendredi de :<br>8h30 à 12h et 14h à 18h30 </small>
                    <small class="text-light"><br>Et le samedi de 10h à 18h.</small>

                </div>
                <!-- Grid column -->

                <hr class="clearfix w-100 d-md-none">

                <!-- Grid column -->
                <div class="col-md-3 mx-auto">

                    <!-- Links -->
                    <h5 class="font-weight-bold text-uppercase mt-3 mb-2 text-light">Nos Agences</h5>

                    <ul class="list-unstyled">
                        <li>
                            <p class="text-light">Lille</p>
                        </li>
                        <li>
                            <p class="text-light">Paris</p>
                        </li>
                    </ul>

                </div>
                <!-- Grid column -->

                <hr class="clearfix w-100 d-md-none">

                <!-- Grid column -->
                <div class="col-md-3 mx-auto">

                    <!-- Links -->
                    <h5 class="font-weight-bold text-uppercase mt-3 mb-2 text-light">Nous retrouver</h5>
                    <ul class="list-unstyled">
                        <li>
                            <a href="https://facebook.com" target="_blank" class="text-light"><i class="fab fa-2x fa-facebook-square text-light"></i>&nbsp;Immo 2020</a>
                        </li>
                        <li>
                            <p class="text-light">39 rue nationale<br>59000 LILLE</p>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- Grid column -->

        <hr class="clearfix w-100 d-md-none">

        <!-- Copyright -->
        <div class="footer-copyright text-center py-1 text-light">© 2020 Copyright:
            <a href="{{ path('home') }}" class="text-light"> Immo2020.com</a>
        </div>
        <!-- Copyright -->

    </footer>
{% endblock %}

<script src="{{ asset("/libs/js/jquery.js") }}"></script>
<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
{% block javascripts %}{% endblock %}
</body>
</html>
